{
  "permissions": [
    "<all_urls>",
    "tabs",
    "contextualIdentities"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content-script.js"],
      "css": ["css/content.css"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "web_accessible_resources": [
    "/img/container-site-d-24.png"
  ],
  "description": "Containers cheerleader tells you about containers being added",
  "homepage_url": "https://github.com/jonathanKingston/extension-debugging",
  "manifest_version": 2,
  "name": "Containers cheerleader",
  "version": "1.0"
}
